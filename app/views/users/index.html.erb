<h1>Listing Users</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th>Points</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= link_to user.name, user %></td>
    <td><%= user.email %></td>
    <td><%= user.points %></td>
  </tr>
<% end %>
</table>

<br/>

<%= link_to 'New User', new_user_path %>
